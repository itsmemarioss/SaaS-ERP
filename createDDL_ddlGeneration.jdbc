CREATE TABLE FUNCIONARIO (ID  SERIAL NOT NULL, empresa_id VARCHAR(31), NOME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE usuario (ID  SERIAL NOT NULL, empresa_id VARCHAR(31), ATIVO BOOLEAN, EMAIL VARCHAR(255) UNIQUE, NOME VARCHAR(255), SENHA VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE EMPRESA (ID  SERIAL NOT NULL, FANTASIA VARCHAR(255), razao_social VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE PRODUTO (ID  SERIAL NOT NULL, empresa_id VARCHAR(31), DESCRICAO VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE Usuario_PERMISSAO (Usuario_ID BIGINT, permissao VARCHAR(50))
ALTER TABLE Usuario_PERMISSAO ADD CONSTRAINT FK_Usuario_PERMISSAO_Usuario_ID FOREIGN KEY (Usuario_ID) REFERENCES usuario (ID)
